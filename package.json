{
  "name": "emmersive",
  "version": "0.1.0",
  "description": "emmersive",
  "dependencies": {
    "babel-preset-es2015": "^6",
    "es6-promise": "^3",
    "firebase": "^3",
    "whatwg-fetch": "^1.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-loader": "^6.2.4",
    "clean-webpack-plugin": "^0.1.9",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "firebase-tools": "^3.0.3",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.21.0",
    "postcss-custom-properties": "^5.0.1",
    "postcss-import": "^8.1.2",
    "postcss-loader": "^0.9.1",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1",
    "webpack-svgstore-plugin": "^2.2.2"
  },
  "scripts": {
    "build:dev": "webpack -d --progress",
    "build:prod": "webpack -p --progress",
    "build:watch": "webpack -d --watch --progress",
    "deploy:login": "firebase login",
    "deploy:logout": "firebase logout",
    "deploy:dev": "npm run build:prod && firebase deploy",
    "deploy:prod": "npm run build:prod && firebase deploy -f flickering-inferno-1351",
    "deploy:staging": "npm run build:prod && firebase deploy -f emmersive-stage",
    "serve": "firebase serve",
    "start": "npm run build:watch | npm run serve"
  },
  "cordovaPlugins": [
    "cordova-plugin-device",
    "cordova-plugin-console",
    "cordova-plugin-whitelist",
    "cordova-plugin-splashscreen",
    "cordova-plugin-statusbar"
  ],
  "cordovaPlatforms": [
    "ios",
    "android"
  ]
}
